#include<stdio.h>
int main (){
    int n ;
    printf("Enter the number:\n");
    scanf("%d",&n);
    if(n>0)
    {
        printf("no is positive");
       
    }
    else
    {
        printf("no is negative");
    }
    getch();
return 0;
}
